@model IEnumerable<CleanTrackerV18.Models.CleanTracker.Units>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UnitName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UnitType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UnitNeighborhood)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UnitStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UnitLastModified)
            </th>
            <th hidden>
                @Html.DisplayNameFor(model => model.UnitLastModifiedBy)
            </th>
            <th hidden>
                @Html.DisplayNameFor(model => model.UnitIsActive)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UnitName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UnitType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UnitNeighborhood)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UnitStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UnitLastModified)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UnitLastModifiedBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UnitIsActive)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.UnitId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.UnitId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.UnitId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
