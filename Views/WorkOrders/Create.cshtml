@model CleanTrackerV18.Models.CleanTracker.WorkOrders

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Work Orders</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div hidden class="form-group">
                <label asp-for="UserId" class="control-label"></label>
                <select asp-for="UserId" class="form-control" asp-items="ViewBag.UserId"></select>
            </div>
            <div hidden class="form-group">
                <label asp-for="UnitId" class="control-label"></label>
                <select asp-for="UnitId" class="form-control" asp-items="ViewBag.UnitId"></select>
            </div>
            <div hidden class="form-group">
                <label asp-for="ServiceId" class="control-label"></label>
                <select asp-for="ServiceId" class="form-control" asp-items="ViewBag.ServiceId"></select>
            </div>
            <!--<script>$('#basic').editableSelect();</script>-->
            <div class="form-group">
                <label asp-for="WorkOrderName" class="control-label"></label>
                <input autofocus type="text" autocomplete="off" asp-for="WorkOrderName" class="form-control" asp-items="ViewBag.UnitId"> <!--UnitId-->
                <span asp-validation-for="WorkOrderName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
